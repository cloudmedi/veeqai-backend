# Database Configuration
MONGODB_URI=mongodb://localhost:27017/veeqai
REDIS_URL=redis://localhost:6379
JWT_SECRET=your-secret-key-here
JWT_REFRESH_SECRET=your-refresh-secret-key-here
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d
NODE_ENV=development

# Frontend Configuration
FRONTEND_URL=http://localhost:5173

# Replicate API Configuration
REPLICATE_API_TOKEN=your_replicate_token_here

# MusicGPT API Configuration
MUSICGPT_API_KEY=your_musicgpt_api_key_here

# Cloudflare R2 Configuration
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_ACCESS_KEY_ID=your_cloudflare_access_key
CLOUDFLARE_SECRET_ACCESS_KEY=your_cloudflare_secret_key
CDN_PROVIDER=cloudflare
CLOUDFLARE_BUCKET_NAME=your_bucket_name
CLOUDFLARE_CUSTOM_DOMAIN=your_custom_domain

# File Upload Configuration
UPLOAD_PATH=VeeqAI/Music
MAX_FILE_SIZE=50MB

# Iyzico Payment Configuration
IYZICO_API_KEY=your_iyzico_api_key
IYZICO_SECRET_KEY=your_iyzico_secret_key
IYZICO_BASE_URL=https://sandbox-api.iyzipay.com
IYZICO_WEBHOOK_SECRET=your_webhook_secret

# Payment URLs
PAYMENT_SUCCESS_URL=https://app.veeq.ai/payment/success
PAYMENT_FAILURE_URL=https://app.veeq.ai/payment/failed
PAYMENT_CALLBACK_URL=https://api.veeq.ai/api/payment/callback